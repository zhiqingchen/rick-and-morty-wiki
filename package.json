{
	"name": "rick-and-morty-wiki",
	"version": "1.0.0",
	"private": true,
	"description": "Rick and Morty wiki based on rick-and-morty-api",
	"templateInfo": {
		"name": "redux",
		"typescript": true,
		"css": "less"
	},
	"scripts": {
		"dev": "npm run build:weapp -- --watch",
		"build": "taro build --type weapp",
		"ios1": "npm run build:rn -- --watch",
		"ios2": "curl -I http://localhost:8081/index.bundle?platform=ios&dev=true -o /dev/null",
		"ios3": "cd taro-native-shell  &&  npm run ios",
		"and1": "npm run build:rn -- --watch",
		"and2": "curl -I http://localhost:8081/index.bundle?platform=android&dev=true -o /dev/null",
		"and3": "cd taro-native-shell  &&  npm run android ",
		"podinstall": "cd taro-native-shell/ios  &&  pod install ",
		"icon": "npx iconfont-taro --config src/assets/iconfont.json",
		"build:android": "taro build --type rn --platform android  &&  cd taro-native-shell/android  &&  ./gradlew assembleRelease --warning-mode all",
		"build:ios": "taro build --type rn --platform ios",
		"build:weapp": "taro build --type weapp",
		"build:swan": "taro build --type swan",
		"build:alipay": "taro build --type alipay",
		"build:tt": "taro build --type tt",
		"build:h5": "taro build --type h5",
		"build:qq": "taro build --type qq",
		"build:jd": "taro build --type jd",
		"build:rn": "taro build --type rn",
		"build:quickapp": "taro build --type quickapp",
		"dev:weapp": "npm run build:weapp -- --watch",
		"dev:swan": "npm run build:swan -- --watch",
		"dev:alipay": "npm run build:alipay -- --watch",
		"dev:rn": "npm run build:rn -- --watch --qr",
		"dev:tt": "npm run build:tt -- --watch",
		"dev:h5": "npm run build:h5 -- --watch",
		"dev:qq": "npm run build:qq -- --watch",
		"dev:jd": "npm run build:jd -- --watch",
		"dev:quickapp": "npm run build:quickapp -- --watch"
	},
	"browserslist": [
		"last 3 versions",
		"Android >= 4.1",
		"ios >= 8"
	],
	"author": "",
	"license": "MIT",
	"dependencies": {
		"@babel/runtime": "^7.7.7",
		"@tarojs/components": "3.5.3-alpha.0",
		"@tarojs/plugin-framework-react": "3.5.3-alpha.0",
		"@tarojs/react": "3.5.3-alpha.0",
		"@tarojs/rn-runner": "3.5.3-alpha.0",
		"@tarojs/runtime": "3.5.3-alpha.0",
		"@tarojs/taro": "3.5.3-alpha.0",
		"@tarojs/taro-rn": "3.5.3-alpha.0",
		"classnames": "^2.3.1",
		"lodash": "^4.17.21",
		"react": "^18.0.0",
		"react-dom": "^18.0.0",
		"react-native": "^0.69.3",
		"react-redux": "^7.2.0",
		"redux": "^4.0.0",
		"redux-logger": "^3.0.6",
		"redux-thunk": "^2.3.0"
	},
	"devDependencies": {
		"@babel/core": "^7.8.0",
		"@tarojs/cli": "3.5.3-alpha.0",
		"@tarojs/mini-runner": "3.5.3-alpha.0",
		"@tarojs/webpack-runner": "3.5.3-alpha.0",
		"@types/react": "^18.0.0",
		"@types/webpack-env": "^1.13.6",
		"@typescript-eslint/eslint-plugin": "^4.23.0",
		"@typescript-eslint/parser": "^4.23.0",
		"babel-preset-taro": "3.5.3-alpha.0",
		"eslint": "^6.8.0",
		"eslint-config-taro": "3.5.3-alpha.0",
		"eslint-plugin-import": "^2.12.0",
		"eslint-plugin-react": "^7.8.2",
		"eslint-plugin-react-hooks": "^1.6.1",
		"stylelint": "9.3.0",
		"taro-iconfont-cli": "^3.2.1",
		"typescript": "^3.7.0"
	}
}
